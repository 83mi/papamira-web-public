<div class="container">
  <h1>
    <%=
      tclass = "badge-warning"
      "<b class=\"badge #{tclass}\">キャラクター選択</b>"
    %></h1>
  <div class="form-group has-success">
    <div class="navbar-form" role="search">
      <select name="select" class="form-control" id="server-id" onChange="getSelect(0,this.value)">
        <option value="s">S鯖</option>
        <option value="b">B鯖</option>
        <option value="v">V鯖</option>
        <option value="g">G鯖</option>
      </select>
      <input id="word" type="text" class="form-control" name="word" size="60" placeholder="キャラの検索"/>
      <button id="search" type="button" class="btn btn-success">検索</button>
    </div>
  </div>
  <div id="loading"></div>
  <div id="result">
    <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>名前</th>
        <th>レベル</th>
        <th>情報</th>
      </tr>
    </thead>
    <tbody>
      <%= @body %>
    </tbody>
    </table>
  </div>
</div>
<script src="/js/jquery-ui.min.js"></script>
<script src="/js/jquery.cookie.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/scrolltopcontrol.js"></script>
<script src="/js/game.js"></script>
